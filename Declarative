pipeline{
	agent any
	tools {
 		 maven 'MAVEN_HOME'
	}

	stages{
		stage('SCM Checkout'){
			steps{
				git branch: 'main', credentialsId: 'git_cred', url: 'https://github.com/riteshalu/demo.git'
			}
		}
		stage('Build'){
			steps{
				sh 'mvn package'
			}
		}
		stage('Test'){
			steps{
				sh 'mvn test'
			}
		}
		stage('Deploy'){
			steps{
				sh 'mvn clean'
			}
		}
	}
}
