pipeline{
	agent any
	stages{
		stage('SCM Checkout'){
			steps{
				echo "Running SCM Checkout job"
			}
		}
		stage('Build'){
			steps{
				echo "Running Build job"
			}
		}
		stage('Test'){
			steps{
				echo "Running Test job"
			}
		}
		stage('Deploy'){
			steps{
				echo "Running Deploy job"
			}
		}
	}
}
